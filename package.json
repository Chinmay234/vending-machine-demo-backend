{
  "name": "vending-machine",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "cross-env NODE_ENV=development nodemon -i \"*.test.js\" app.js",
    "prod": "cross-env NODE_ENV=production node .",
    "lint": "eslint src",
    "docs": "apidoc -i src -o docs && apidoc-markdown -p docs -o DOCS.md",
    "postdocs": "open-cli docs/index.html"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-joi-validation": "^5.0.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "joi": "^17.5.0",
    "morgan": "~1.9.1",
    "mysql2": "^3.6.3"
  },
  "devDependencies": {
    "apidoc": "^0.20.0",
    "apidoc-markdown": "^0.2.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.0",
    "eslint-plugin-standard": "^4.0.1",
    "jest-cli": "^25.1.0",
    "lodash": "^4.17.21",
    "nodemon": "^3.0.1",
    "open-cli": "^6.0.0"
  }
